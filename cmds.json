[
  {
    "key": "KEY_UP",
    "desc": "go up",
    "do": "rc[0]-1",
    "undo": "rc[0]+1"
  },
  {
    "key": "KEY_DOWN",
    "desc": "go down",
    "do": "rc[0]+1",
    "undo": "rc[0]-1"
  },
  {
    "key": "KEY_RIGHT",
    "desc": "go right",
    "do": "rc[1]+1",
    "undo": "rc[1]-1"
  },
  {
    "key": "KEY_LEFT",
    "desc": "go left",
    "do": "rc[1]-1",
    "undo": "rc[1]+1"
  },
  {
    "key": "d",
    "desc": "flush cell",
    "do": "cell.set('')",
    "undo": "cell.set(value)"
  },
  {
    "key": "q",
    "desc": "close last sheet",
    "do": "sheets.pop()",
    "undo": "sheets.append(sheet)"
  },
  {
    "key": "u",
    "desc": "undo",
    "do": "exec(lastcmd['undo'], globals(), lastcmd['locals'])",
    "undo": "exec(lastcmd['do'], globals(), lastcmd['locals'])"
  },
  {
    "key": "H",
    "desc": "help",
    "do": "openkeymap()",
    "undo": "closekeymap()"
  },
  {
    "key": "U",
    "desc": "undo history",
    "do": "opencommands()",
    "undo": "closehelp"
  },
  {
    "key": "e",
    "desc": "edit cell",
    "do": "cell.set(window.getstr().decode('utf-8'))",
    "undo": "cell.set(value)"
  }
]
